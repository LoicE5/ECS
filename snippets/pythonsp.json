{
    "/ comment Python": {
        "prefix": "/",
        "body": ["# $1 "],
        "description": "Regular comment"
    },
        "/ ⬆ comment Python": {
        "prefix": "/up",
        "body": ["#⬆ $1 ⬆"],
        "description": "Comment the code above"
    },
        "/ ⬇ comment Python": {
        "prefix": "/down",
        "body": ["#⬇ $1 ⬇"],
        "description": "Comment the code below"
    },
    "/ comment Python Legacy": {
        "prefix": "/c",
        "body": ["# $1 "],
        "description": "Regular comment - Legacy"
    }
}